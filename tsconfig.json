{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "files": ["src/vue-serial.ts"],
  "include": ["node_modules/@types/**/*.d.ts"],
  "compilerOptions": {
    "outDir": "./dist"
  },
  "typedocOptions": {
    "entryPoints": ["src/vue-serial.ts"],
    "out": "./api",
    "plugin": ["typedoc-plugin-markdown", "typedoc-github-wiki-theme"],
    "publicPath": "./",
    "theme": "github-wiki",
    "readme": "none",
    "excludePrivate": true,
    "excludeExternals": true,
    "disableSources": true
  }
}